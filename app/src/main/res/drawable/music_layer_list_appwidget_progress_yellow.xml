<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#1AFFFFFF" />

            <stroke
                android:dashGap="0.0dip"
                android:dashWidth="0.0dip"
                android:width="0.0dip"
                android:color="#FFFFFF" />

            <corners
                android:bottomLeftRadius="0.0dip"
                android:bottomRightRadius="0.0dip"
                android:topLeftRadius="0.0dip"
                android:topRightRadius="0.0dip" />

            <padding
                android:bottom="0.0dip"
                android:left="0.0dip"
                android:right="0.0dip"
                android:top="0.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#FFCC00" />

                <stroke
                    android:dashGap="0.0dip"
                    android:dashWidth="0.0dip"
                    android:width="0.0dip"
                    android:color="#FFFFFF" />

                <corners
                    android:bottomLeftRadius="0.0dip"
                    android:bottomRightRadius="0.0dip"
                    android:topLeftRadius="0.0dip"
                    android:topRightRadius="0.0dip" />

                <padding
                    android:bottom="0.0dip"
                    android:left="0.0dip"
                    android:right="0.0dip"
                    android:top="0.0dip" />
            </shape>
        </clip>
    </item>

</layer-list>